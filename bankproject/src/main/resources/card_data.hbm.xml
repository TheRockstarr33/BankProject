<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name = "com.hibernate.maven.backend.databaseInfo.CardData" table = "card_data">
        <id name="id" type="int">
            <column name="id" />
            <generator class="incriment" />  <!--Necessary?-->
        </id>

        <property name="userId" type="int">
            <column name="user_id"/>
        </property>

        <property name="cardId" type="int">
            <column name="card_id"/>
        </property>

        <property name="pinNum" type="int">
            <column name="pin_number"/>
        </property>

        <property name="signupDate" type="timestamp">
            <column name="signup_date"/>
        </property>
    </class>


</hibernate-mapping>